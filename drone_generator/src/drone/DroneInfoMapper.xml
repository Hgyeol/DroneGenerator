<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="drone/DroneMapper">
	<insert id="save" parameterType="drnoe/DroneInfo" timeout="5">
		insert into drone_info (drone_id, max_area) values(#{droneId}, #{maxArea})
	</insert>
	<select id="findAll" resultType="drone/DroneInfo" timeout="5"></select>
</mapper>